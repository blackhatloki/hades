<!--   Copyright (c) 2003 Hewlett-Packard Development Company, L.P.  -->

<!-- Description:  This is a sample XML script to return the status  -->
<!--               of the Virtual Power Button cable that may be     -->
<!--               connected to a Remote Insight Lights-Out Edition  -->
<!--               (RILOE II) board.                                 -->

<!-- NOTE:  You will need to replace the USER_LOGIN, PASSWORD and    -->
<!--        IMAGE_LOCATION fields with values that are appropriate   -->
<!--        for your environment.                                    -->

<!--        Use CPQLOCFG.EXE ver 2.10 or greater with this script    -->
<!--        This script was written for RILOE II fw ver 1.10 or      -->
<!--        higher.                                                  -->

<!--        See the RILOE II User Guide for more information on      -->
<!--        scripting and the syntax of the RIBCL XML.               -->

<!--        Firmware support infomation for this script:             -->
<!--            iLO 2 - None                                         -->
<!--              iLO - None                                         -->
<!--         RILOE II - Version 1.10 or later.                       -->

<RIBCL VERSION="2.0">
  <LOGIN USER_LOGIN="adminname" PASSWORD="password">
  <SERVER_INFO MODE="read">
    <GET_VPB_CABLE_STATUS/>
  </SERVER_INFO>
  </LOGIN>
</RIBCL>

